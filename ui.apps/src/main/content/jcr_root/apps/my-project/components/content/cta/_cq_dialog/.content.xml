<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="CTA"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            margin="false"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <featureconfig
                jcr:primaryType="nt:unstructured"
                jcr:title="CTA"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <buttonLinkUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Button Link URL"
                                name="./buttonLinkUrl"
                                rootPath="/content"/>
                            <buttonLinkLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Button Link Label"
                                name="./buttonLinkLabel"/>
                            <buttonStyle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Button Style"
                                name="./buttonStyle">
                                <items jcr:primaryType="nt:unstructured">
                                    <none
                                        jcr:primaryType="nt:unstructured"
                                        text="None"
                                        value="btn-default"/>
                                    <primary
                                        jcr:primaryType="nt:unstructured"
                                        text="Primary"
                                        value="btn-primary"/>
                                </items>
                            </buttonStyle>
                            <buttonLinkStyle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Button Link Style"
                                name="./buttonLinkStyle">
                                <items jcr:primaryType="nt:unstructured">
                                    <internal
                                        jcr:primaryType="nt:unstructured"
                                        text="Internal"
                                        value="fa-angle-right"/>
                                    <external
                                        jcr:primaryType="nt:unstructured"
                                        text="External"
                                        value="fa-external-link-square"/>
                                    <download
                                        jcr:primaryType="nt:unstructured"
                                        text="Downloads"
                                        value="fa-cloud-download"/>
                                </items>
                            </buttonLinkStyle>
                            <buttonLinkTooltip
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Button Link Tooltip"
                                name="./buttonLinkToolTip"/>
                            <buttonLinkTarget
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Check the box to open in new window"
                                name="./buttonLinkTarget"
                                text="Open in New Window"
                                value="_blank"/>
                        </items>
                    </columns>
                </items>
            </featureconfig>
        </items>
    </content>
</jcr:root>
